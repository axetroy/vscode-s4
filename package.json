{
  "name": "s4",
  "displayName": "s4",
  "description": "vscode language support for s4",
  "version": "0.1.0",
  "publisher": "axetroy",
  "engines": {
    "vscode": "^1.40.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/axetroy/vscode-s4.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/axetroy/vscode-s4/issues"
  },
  "homepage": "https://github.com/axetroy/vscode-s4#readme",
  "keywords": [
    "schema",
    "json",
    "vscode",
    "extension"
  ],
  "categories": [
    "Other"
  ],
  "contributes": {
    "languages": [
      {
        "id": "s4",
        "extensions": [
          ".s4"
        ],
        "filenames": [
          "s4"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "s4",
        "scopeName": "source.s4",
        "path": "./syntaxes/s4.tmLanguage.json"
      }
    ]
  }
}
